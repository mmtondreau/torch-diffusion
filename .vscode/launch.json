  "version": "0.2.0",
  "configurations": [
    {
      "name": "Training",
      "type": "python",
      "request": "launch",
      "module": "torch_diffusion",
      "justMyCode": true,
      "env": {
        "MODEL_HEIGHT": "48",
        "MODEL_WIDTH": "32",
        "TRAINING_MAX_EPOCHS": "100",
        "TRAINING_BATCH_SIZE": "32",
        "PREPROCESS_OUTPUT_DIR": "./preprocessed_dir",
        "PREPROCESS_INPUT_DIR": "./data"
      }
    },
    {
      "name": "Preprocess",
      "type": "python",
      "request": "launch",
      "module": "torch_diffusion",
      "justMyCode": true,
      "env": {
        "MODEL_HEIGHT": "48",
        "MODEL_WIDTH": "32",
        "TRAINING_MAX_EPOCHS": "100",
        "TRAINING_BATCH_SIZE": "32",
        "PREPROCESS_OUTPUT_DIR": "./preprocessed_dir",
        "PREPROCESS_INPUT_DIR": "./data",
        "PREPROCESS_ENABLED": "True"
      }
    },
    {
      "name": "Inference",
      "type": "python",
      "request": "launch",
      "module": "torch_diffusion",
      "justMyCode": true,
      "env": {
        "MODEL_HEIGHT": "48",
        "MODEL_WIDTH": "32",
        "INFERENCE_ENABLED": "True"
      }
    }
  ]
}
